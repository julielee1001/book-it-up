<h1> <%= current_user.first_name %>'s Lists</h1>

<% if current_user.lists.present? %>
            <% current_user.lists.each do |list| %>
                <div>
                    <p><%= link_to list.name, list_path(list) %></p>
                </div>
            <% end %>
        <% else %>
            <p>No Lists Found</p>
<%end%>

<br>


<%= link_to "New List", new_list_path %>
    